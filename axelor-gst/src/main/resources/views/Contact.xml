<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
	
	<form model="com.axelor.apps.base.db.Partner" title="Contact-form" name="contact-form" id="partner-form-gst-contact-include">
		<panel>
			<field name="name" required="true"></field>
			<field name="gstContactType" selection="contact.gst.type.select" required="true"></field>
			<field name="primaryEmail"></field>
			<field name="secondaryEmail"></field>
			<field name="primaryPhone"></field>
			<field name="secondaryPhone"></field>
		</panel>
	</form>
	<grid model="com.axelor.apps.base.db.Partner" title="Contact-Grid" name="contact-grid">
		<field name="name"></field>
		<field name="gstContactType"></field>
		<field name="primaryEmail"></field>
		<field name="secondaryEmail"></field>
		<field name="primaryPhone"></field>
		<field name="secondaryEmail"></field>
	</grid>
</object-views>